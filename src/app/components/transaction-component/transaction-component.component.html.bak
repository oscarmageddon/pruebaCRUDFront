<div class="row">
    <div class="col"></div>
    <div class="col-md-10">
        <div class="row justify-content-md-center">

            <h2 class="mt-5">Gestion de Transaccion
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-chevron-compact-right" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M6.776 1.553a.5.5 0 0 1 .671.223l3 6a.5.5 0 0 1 0 .448l-3 6a.5.5 0 1 1-.894-.448L9.44 8 6.553 2.224a.5.5 0 0 1 .223-.671z"/>
              </svg></h2>
        </div>


        <form>

            <div class="card">
                <div class="card-body">
                    <div class="mb-3">
                        <br><br>
                        <input type="text" name="nomUser" class="form-control" [(ngModel)]="nomUser"
                            placeholder="Nombre..." /><br>
                        <input type="text" name="apeUser" class="form-control" [(ngModel)]="apeUser"
                            placeholder="Apellido..." /><br>
                        <input type="text" name="dniUser" class="form-control" [(ngModel)]="dniUser"
                            placeholder="Rut..." /><br>
                        <input type="text" name="payMethod" class="form-control" [(ngModel)]="payMethod"
                            placeholder="Metodo de pago..." />
                        <br>
                        <input type="button" value="Agregar" (click)="guardar()" class="btn btn-primary">
                    </div>
                </div>
            </div>


        </form>
        <br>
        <div class="row mg-t-12 mg-b-12 mg-l-12 mg-b-10">
            <div class="col-lg-5">
                <div class="search-box pull-left" style="width: 250px !important;">
                    <input type="text" class="form-control" placeholder="Buscar">


                </div>
                <!---->

            </div>

        </div>
        <br>
        <table class="table table-responsive table-hover">
            <thead>
                <tr class="primary">
                    <th scope="col"></th>
                    <th scope="col">Nombre</th>
                    <th scope="col">Apellido</th>
                    <th scope="col">Rut</th>
                    <th scope="col">Metodo de Pago</th>
                    <th scope="col">Estado</th>
                    <th scope="col">

                    </th>
                    <th scope="col">

                    </th>
                </tr>
            </thead>
            <tbody>
              <tr >
                    <th scope="row" class="text-center"></th>
                        <td class="text-center"></td>
                        <td class="text-center"></td>
                        <td class="text-center"></td>
                        <td class="text-center"></td>
                        <td class="text-center"></td>
                    <th class="text-center" scope="col"><input type="button" value="Editar" data-bs-toggle="modal" data-bs-target="#exampleModal" class="btn btn-success"></th>
                    <th  class="text-center" scope="col"><input type="button" value="Eliminar" class="btn btn-danger"></th>
                </tr>


            </tbody>
        </table>
    </div>
 
    <div class="col"></div>
</div>